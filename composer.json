{
    "name": "stev/doctrine-translations-bundle",
    "description": "Symfony 2 Bundle providing form data mappers to handle translations using doctrine entities",
    "type": "symfony-bundle",
    "authors": [{
            "name": "Stefan Matei",
            "email": "stev.matei@gmail.com"
        }],
    "keywords": [
        "symfony doctrine translations bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^2.6|^3.0",
        "doctrine/orm": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Stev\\DoctrineTranslationsBundle\\": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
